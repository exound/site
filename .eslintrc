{
  "ecmaFeatures": {
    "restParams": true,
    "blockBindings": true,
    "forOf": true,
    "jsx": true,
    "modules": true,
    "classes": true,
    "arrowFunctions": true,
    "defaultParams": true,
    "destructuring": true,
    "generators": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "superInFunctions": true,
    "templateStrings": true
  },
  "rules": {
    "semi": [2, "always"]
  }
}
